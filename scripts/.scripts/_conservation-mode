#compdef conservation-mode

_conservation-mode(){
  local -a commands

  commands=(
    "on:Turn on conservation mode"
    "off:Turn off conservation mode"
  )

  _arguments -C \
    '1:cmd:->cmds'

  case "$state" in
    (cmds)
      _describe -t commands 'commands' commands
      ;;
  esac
}

_conservation-mode
