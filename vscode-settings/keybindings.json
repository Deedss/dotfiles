// Place your key bindings in this file to override the defaultsauto[]
[
  { // Disable find and replace
    "key": "ctrl+h",
    "command": "-editor.action.startFindReplaceAction",
    "when": "editorFocus || editorIsOpen"
  },
  { // Disable closing of all windows
    "key": "ctrl+shift+w",
    "command": "-workbench.action.closeWindow"
  },
  { // Close files explorer
    "key": "ctrl+shift+e",
    "command": "workbench.action.toggleSidebarVisibility",
    "when": "activeViewlet == 'workbench.view.explorer' && !editorFocus",
  },
  { // Close Search 
    "key": "ctrl+shift+f",
    "command": "workbench.action.toggleSidebarVisibility",
    "when": "activeViewlet == 'workbench.view.search' && !editorFocus",
  },
  { // Close SCM
    "key": "ctrl+shift+g",
    "command": "workbench.action.toggleSidebarVisibility",
    "when": "activeViewlet == 'workbench.view.scm' && !editorFocus",
  },
  { // Close Debug 
    "key": "ctrl+shift+d",
    "command": "workbench.action.toggleSidebarVisibility",
    "when": "activeViewlet == 'workbench.view.debug' && !editorFocus",
  },
  { // Move back to editor from sidebar
    "key": "ctrl+w h",
    "command": "workbench.action.focusActiveEditorGroup",
    "when": "sideBarFocus"
  },
  { // rename file
    "key": "r",
    "command": "renameFile",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  { // create new file
    "key": "a",
    "command": "explorer.newFile",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  { // create new folder
    "key": "shift+a",
    "command": "explorer.newFolder",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "key": "ctrl+n",
    "command": "-workbench.action.files.newUntitledFile"
  },
  {
    "key": "escape",
    "command": "workbench.action.toggleSidebarVisibility",
    "when": "sideBarFocus"
  }
]